
<%-include("header")-%>
  <div class="box" id="heading">
    <h1> <%= listTitle %> </h1>
  </div>
  <div class="box">

    <% for(var i=0;i < ITEMS.length ; i++){%>
    <div class="item">
      <input type="checkbox">
      <p> <%=ITEMS[i]%> </p>
    </div>


    <%}%>




  <form class="item" action="/" method="post">
    <input type="text" name="items" placeholder="New Item" autocomplete="off">
    <button type="submit" name="submit" value=<%= listTitle %>>+</button>
  </form>
  </div>
<%-include("footer")-%>
